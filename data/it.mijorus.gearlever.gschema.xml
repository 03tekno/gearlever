<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gearlever">
    <schema id="it.mijorus.gearlever" path="/it/mijorus/gearlever/">
        <key name="appimages-default-folder" type="s">
            <default>"~/AppImages"</default>
        </key>
        <key name="move-appimage-on-integration" type="b">
            <default>true</default>
        </key>
        <key name="exec-as-name-for-terminal-apps" type="b">
            <default>false</default>
        </key>
        <key name="simple-file-name-for-apps" type="b">
            <default>false</default>
        </key>
        <key name="fetch-updates-on-startup" type="b">
            <default>true</default>
        </key>
        <key name="debug-logs" type="b">
            <default>false</default>
        </key>
        <key name="first-run" type="b">
            <default>true</default>
        </key>
        <key name="first-run-v2" type="b">
            <default>true</default>
        </key>
        <key name="last-run-version" type="s">
            <default>"1.0.0"</default>
        </key>
        <key name="manage-files-outside-default-folder" type="b">
            <default>true</default>
        </key>
        <key name="is-maximized" type="b">
            <default>false</default>
        </key>
    </schema>
</schemalist>